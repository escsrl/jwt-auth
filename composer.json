{
    "name": "escsrl/symfony-jwt-auth",
    "type": "library",
    "description": "Symfony jwt auth bundle. Comes with refresh token enabled by default",
    "keywords": ["jwt", "auth", "symfony-auth"],
    "license": "MIT",
    "authors": [
        {
            "name": "Alessandro Bellanda",
            "email": "a.bellanda@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Esc\\JwtAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Esc\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "doctrine/doctrine-bundle": "^1.11",
        "doctrine/orm": "^2.6",
        "gesdinet/jwt-refresh-token-bundle": "^0.7.1",
        "lexik/jwt-authentication-bundle": "^2.6",
        "nesbot/carbon": "^2.24",
        "symfony/http-foundation": "^4.3",
        "symfony/security": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1"
    },
    "config": {
        "sort-packages": true
    }
}
