{
    "name": "escsrl/jwt-auth",
    "type": "library",
    "description": "Symfony jwt auth bundle. Comes with refresh token enabled by default",
    "keywords": ["jwt", "auth", "symfony-auth"],
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:escsrl/JWTRefreshTokenBundle.git"
        }
    ],
    "authors": [
        {
            "name": "Alessandro Bellanda",
            "email": "a.bellanda@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Esc\\JwtAuth\\": "src/"
        }
    },
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "escsrl/user": "dev-master",
        "gesdinet/jwt-refresh-token-bundle": "dev-master",
        "lexik/jwt-authentication-bundle": "^2.6",
        "symfony/http-foundation": "^5.0",
        "symfony/security-bundle": "^5.0"
    },
    "config": {
        "sort-packages": true
    }
}
